			public const VS_CADASTRO:uint = 1;
			
			private var dataFieldCodigo:String = "codigo";
			
			private var dataFieldNome:String = "nome";
			
			private var dataFieldTipo:String = "tipo";
			
			private var dataFieldAtivo:String = "ativo";
			
